datasource postgres {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            String   @id @default(cuid())
  creatTime     DateTime @default(now())
  lastLoginTime DateTime @updatedAt
  trueName      String?
  username      String   @unique
  password      String
  roles         Json
}
